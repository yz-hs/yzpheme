<% if(theme.site_warning.enable){ %>
	<div class="text-alert <%= theme.site_warning.type %>" style="padding: 15px; margin: 0px; border-radius: 0px;<% if(theme.site_warning.bgcolor){ %>background-color: #<%- theme.site_warning.bgcolor %> <% } %> <% if(theme.site_warning.color){ %>color: #<%- theme.site_warning.color %> <% } %> ">
		<%- theme.site_warning.text %>
	</div>
<% } %>