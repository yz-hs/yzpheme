<script type="text/javascript">
    function santacoloredegg() {
        document.getElementById("santa-colored-egg").innerHTML = 
        "<div class=\"santa\" onclick=\"santaremove()\">"+
        "<div class=\"santa-body\" style=\"font-size: 500px;\">"+
        "<div class=\"santa-head\">"+
        "<div class=\"santa-ear\"></div>"+
        "<div class=\"santa-ear\"></div>"+
        "<div class=\"santa-hat\"></div>"+
        "<div class=\"santa-eye\"></div>"+
        "<div class=\"santa-eye\"></div>"+
        "<div class=\"santa-nose\"></div>"+
        "<div class=\"santa-mouth\"></div>"+
        "</div></div></div>";
        swal.fire("注意","由于你触碰了圣诞老人，他快气炸了！\n快快刷新让他平静下来！\n若再触碰，他会气炸的 qwq！");
    }

    function santaremove() {
        document.getElementById("santa-colored-egg").innerHTML = "";
        swal.fire("注意","圣诞老人已气炸。 R.I.P.\n同时页面效果清除一些。")
        $("canvas").hide();
        $("meting-js").hide();
